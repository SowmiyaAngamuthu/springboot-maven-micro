pipeline {
    agent any
    
    
    stages{
        stage("Checkout the project") {
           steps{
               git branch: 'master', url: 'https://github.com/SowmiyaAngamuthu/springboot-maven-micro.git'
           } 
        }
        stage("Build the package"){
            steps {
                sh 'mvn clean package'
            }
        }
		
}
}
